            <VBox fx:id="EnterVBox">
               <children>
                  <Label fx:id="EnterLabel" text="Введите свой никнейм" />
                  <TextField fx:id="EnterField" />
               </children>
            </VBox>
            <VBox fx:id="ButtonsVBox" spacing="5.0">
               <children>
                  <Button fx:id="OkButton" mnemonicParsing="false" prefWidth="58.0" text="Ок">
                  </Button>
                  <Button fx:id="CloseButton" mnemonicParsing="false" text="Отмена" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
